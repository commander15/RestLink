{
    "resources": {
        "categories": {
            "fillable": [
                "name",
                "description"
            ],
            "local_key": "category_id",
            "primary_key": "id",
            "relations": {
                "products": {
                    "foreign_key": "category_id",
                    "primary_key": "id",
                    "table": "Products",
                    "type": "HasMany"
                }
            },
            "table": "Categories"
        },
        "products": {
            "fillable": [
                "name",
                "description",
                "price",
                "barcode"
            ],
            "local_key": "product_id",
            "primary_key": "id",
            "relations": {
                "category": {
                    "foreign_key": "category_id",
                    "load": {
                        "auto": true
                    },
                    "primary_key": "id",
                    "table": "Categories",
                    "type": "BelongsToOne"
                },
                "stock": {
                    "foreign_key": "stock_id",
                    "load": {
                        "auto": true
                    },
                    "primary_key": "id",
                    "table": "Stocks",
                    "type": "HasOne"
                }
            },
            "table": "Products"
        },
        "sale_items": {
            "fillable": [
                "quantity"
            ],
            "local_key": "item_id",
            "primary_key": "id",
            "ralations": {
                "product": {
                    "foreign_key": "product_id",
                    "primary_key": "id",
                    "table": "Products",
                    "type": "BelongsToOne"
                }
            },
            "relations": {
                "sale": {
                    "foreign_key": "sale_id",
                    "pivot": "SaleItems",
                    "table": "Sales",
                    "type": "BelongsToMany"
                }
            },
            "table": "SaleItems"
        },
        "sales": {
            "fillable": [
                "number",
                "amount"
            ],
            "local_key": "sale_id",
            "primary_key": "id",
            "relations": {
                "items": {
                    "foreign_key": "sale_id",
                    "primary_key": "id",
                    "table": "SaleItems",
                    "type": "HasMany"
                },
                "seller": {
                    "foreign_key": "seller_id",
                    "load": {
                        "auto": true
                    },
                    "primary_key": "id",
                    "table": "Users",
                    "type": "BelongsToOne"
                }
            },
            "table": "Sales"
        },
        "stocks": {
            "fillable": [
                "quantity"
            ],
            "local_key": "stock_id",
            "primary_key": "id",
            "relations": {
                "product": {
                    "foreign_key": "product_id",
                    "primary_key": "id",
                    "table": "Products",
                    "type": "BelongsToOne"
                }
            },
            "table": "Stocks"
        },
        "user_groups": {
            "fillable": [
                "name"
            ],
            "local_key": "group_id",
            "primary_key": "id",
            "table": "UserGroups"
        },
        "user_roles": {
            "fillable": [
                "name"
            ],
            "local_key": "role_id",
            "primary_key": "id",
            "relations": {
                "users": {
                    "foreign_key": "role_id",
                    "primary_key": "id",
                    "table": "Users",
                    "type": "HasMany"
                }
            },
            "table": "UserRoles"
        },
        "users": {
            "fillable": [
                "name",
                "email",
                "password"
            ],
            "local_key": "user_id",
            "primary_key": "id",
            "relations": {
                "groups": {
                    "foreign_key": "group_id",
                    "pivot": "UserGroupMembers",
                    "primary_key": "user_id",
                    "table": "UserGroups",
                    "type": "BelongsToMany"
                },
                "role": {
                    "foreign_key": "role_id",
                    "primary_key": "id",
                    "table": "UserRoles",
                    "type": "BelongsToOne"
                },
                "sales": {
                    "foreign_key": "user_id",
                    "primary_key": "id",
                    "table": "Sales",
                    "type": "HasMany"
                }
            },
            "table": "Users"
        }
    }
}
